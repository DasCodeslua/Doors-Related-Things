local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Doors Function Entity Spawner and Items", "DarkTheme")
local Tab = Window:NewTab("Items")
local Section = Tab:NewSection("Items Here")
Section:NewButton("Crucifix Old", "Old Crucifix duhh", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/DasCodeslua/Doors-Related-Things/main/Crucifix"))()
    print("working")
end)
Section:NewButton("Crucifix Hotel+", "Press Q on Keyboard to use it on anything", function()
_G.Uses = 1
_G.Range = 30
_G.OnAnything = true
_G.Fail = false
loadstring(game:HttpGet('https://raw.githubusercontent.com/PenguinManiack/Crucifix/main/Crucifix.lua'))()
    print("Crucifix Hotel+ working")
end)
local Tab = Window:NewTab("Entitys Spawn")
local Section = Tab:NewSection("Entitys spawn lol")
Section:NewButton("A-60 (UnKillable)", "ButtonInfo", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/DasCodeslua/Doors-Related-Things/main/A-60"))()
    print("A-60 spawned successfully")
end)
Section:NewButton("A-60 can kill", "ButtonInfo", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/DasCodeslua/Doors-Related-Things/main/A-60%20can%20kill"))()
    print("A-60 spawned")
end)
Section:NewButton("Depth (Beast) (can kill)", "ButtonInfo", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/DasCodeslua/Doors-Related-Things/main/Depth%20beast%20mode"))()
    print("Clicked")
end)
